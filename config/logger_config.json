{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "file_handler": {
      "class": "logging.FileHandler",
      "formatter": "default",
      "filename": "",
      "mode": "a"
    },
    "console_handler": {
      "class": "logging.StreamHandler",
      "formatter": "default"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["file_handler", "console_handler"]
  }
}
